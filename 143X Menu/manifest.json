{
  "manifest_version": 3,
  "name": "143X Slither.io Mod",
  "version": "1.0",
  "description": "The ultimate Slither.io enhancement - by dxxthly. & waynesg | DSC.GG/143X",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "scripting", "tabs"],
  "host_permissions": [
    "*://slither.io/*",
    "*://*.slither.io/*",
    "*://slither.com/io*",
    "*://*.slither.com/io*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["content.js", "143x-menu.css"],
      "matches": ["<all_urls>"]
    }
  ]
}